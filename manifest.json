{
  "manifest_version": 2,
  "name": "Touch20",
  "description": "Introduces Touch controls to Roll20",
  "version": "1.1.2",
  "permissions": [
    "*://*.roll20.net/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.roll20.net/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "main.js",
    "main.js.map",
    "style.css",
    "style.css.map"
  ]
}
